{
  "name": "blog",
  "version": "6.6.6",
  "private": true,
  "scripts": {
    "build": "npm-run-all build:astro build:esbuild",
    "build:astro": "astro build",
    "build:esbuild": "esbuild ./functions/index.ts --bundle --sourcemap --outfile=./dist/worker.mjs --format=esm",
    "clean": "rm -rf dist",
    "start": "npm-run-all -p start:astro start:miniflare",
    "start:astro": "astro dev --verbose",
    "start:miniflare": "miniflare -w -d",
    "start:wrangler": "wrangler dev",
    "preview": "astro preview",
    "publish": "wrangler publish",
    "tail": "wrangler tail"
  },
  "dependencies": {
    "@astrojs/markdown-remark": "^0.6.4",
    "@astrojs/svelte": "^0.1.3",
    "@cloudflare/workers-types": "^3.1.0",
    "@giscus/svelte": "^2.0.2",
    "@okkema/worker": "^0.0.14",
    "@types/node": "^17.0.13",
    "astro": "^1.0.0-beta.31",
    "autoprefixer": "^10.4.2",
    "miniflare": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "rehype-external-links": "^1.0.1",
    "remark-gfm": "^3.0.1",
    "svelte": "^3.48.0",
    "typescript": "^4.4.3",
    "vite-plugin-environment": "^1.1.0",
    "wrangler": "^2.0.6"
  }
}
