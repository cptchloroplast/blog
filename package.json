{
  "name": "blog",
  "version": "6.6.6",
  "private": true,
  "scripts": {
    "build": "npm-run-all build:astro build:webpack",
    "build:astro": "astro build",
    "build:webpack": "webpack -c ./functions/webpack.config.js",
    "build:webpack:watch": "nodemon --watch ./functions --ext ts --ignore *.test.ts --exec npm run build:webpack",
    "clean": "rm -rf dist",
    "start": "npm-run-all -p build:webpack:watch start:astro start:miniflare",
    "start:astro": "astro dev",
    "start:miniflare": "miniflare ./dist/worker.js -w -d -u http://localhost:3000",
    "preview": "astro preview"
  },
  "dependencies": {
    "@astrojs/renderer-svelte": "^0.3.1",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@cloudflare/workers-types": "^3.0.0",
    "@okkema/worker": "^0.0.14",
    "@types/node": "^17.0.13",
    "astro": "^0.22.20",
    "autoprefixer": "^10.4.2",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.2",
    "miniflare": "^1.4.1",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.5",
    "svelte": "^3.43.0",
    "typescript": "^4.4.3",
    "vite-plugin-environment": "^1.1.0",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  }
}
