{
  "name": "blog",
  "version": "6.6.6",
  "private": true,
  "scripts": {
    "prebuild:creds": "scripts/get-credentials.sh",
    "prebuild:bikes": "node scripts/get-bikes.mjs",
    "prebuild": "npm-run-all prebuild:creds prebuild:bikes",
    "build": "astro build",
    "clean": "rm -rf dist",
    "start": "astro dev",
    "start:astro": "astro dev --verbose",
    "start:wrangler": "wrangler pages dev dist --port 5000",
    "preview": "astro preview",
    "publish": "wrangler publish",
    "tail": "wrangler pages deployment tail --project-name blog",
    "pretest": "npm run build",
    "test": "vitest run"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^11.0.1",
    "@astrojs/markdown-remark": "^5.1.1",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/svelte": "^5.6.0",
    "@cloudflare/vitest-pool-workers": "^0.4.11",
    "@cloudflare/workers-types": "^4.20240620.0",
    "@giscus/svelte": "^3.0.0",
    "@okkema/worker": "^2.0.0",
    "astro": "^4.11.3",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.23.1",
    "drizzle-orm": "^0.32.1",
    "googleapis": "^140.0.1",
    "npm-run-all": "^4.1.5",
    "rehype-external-links": "^3.0.0",
    "remark-gfm": "^4.0.0",
    "svelte": "^4.2.18",
    "typescript": "^5.5.2",
    "vite-plugin-environment": "^1.1.3",
    "vitest": "1.5.0",
    "wrangler": "^3.62.0"
  }
}
